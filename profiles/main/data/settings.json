{
    "files.associations": {
        "renv.lock": "json"
    },
    "workbench.colorTheme": "GitHub Dark Default",
    "positron.assistant.enable": true,
    "workbench.iconTheme": "material-icon-theme",
    "files.autoSave": "afterDelay",
    "editor.fontFamily": "\"Jetbrains Mono\"",
    "editor.fontSize": 13,
    "git.autofetch": true,
    "syncSettings.crons": {
        "upload": "0 */4 * * *"  // Upload every 4 hours for backup
    },
    "gitlens.ai.model": "vscode",
    "gitlens.ai.vscode.model": "copilot:gpt-4.1",
    "window.newWindowProfile": "Default",
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "syncSettings.additionalFiles": [
    ],
    "redhat.telemetry.enabled": false,
    "syncSettings.hooks.postDownload": "",
    "workbench.activityBar.location": "top", 
    "latex-workshop.view.pdf.invert": 1.0,
    "workbench.editor.openPositioning": "last",
    "workbench.tree.indent": 10,
    "workbench.editor.enablePreview": false,
    "breadcrumbs.enabled": false,
    "syncSettings.ignoredSettings": [
        
    ],
    "workbench.editor.autoLockGroups": {
        "workbench.editor.positronNotebook": true,
        "jupyter-notebook": true,
        "latex-workshop-pdf-hook": true,
        "workbench.editor.positronDataExplorer": true
    },
    "[markdown]": {
        "editor.quickSuggestions": {
            "other": "on"
        }
    },
    "latex-workshop.view.pdf.zoom": "page-width",
    "workbench.topActionBar.visible": false,
    "workbench.statusBar.visible": false,
    "editor.minimap.enabled": true,
    "terminal.integrated.fontFamily": "MesloLGS Nerd Font"
}

